{
  "name": "@connected-web/terrain-editor",
  "version": "1.0.0",
  "description": "An interactive terrain editor and viewer designed for web using three.js.",
  "main": "index.js",
  "scripts": {
    "packmap": "cd maps/wynnal && zip -r ../wynnal-terrain.wyn legend.json layers/ thumbnails/ locations.json",
    "build": "node ./scripts/build-dist.mjs",
    "build:viewer": "npm --prefix packages/viewer run build",
    "dev:viewer": "npm --prefix packages/viewer run dev -- --host",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/connected-web/terrain-editor.git"
  },
  "keywords": [],
  "author": "Connected Web",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/connected-web/terrain-editor/issues"
  },
  "homepage": "https://github.com/connected-web/terrain-editor#readme",
  "dependencies": {
    "@types/three": "^0.181.0",
    "jszip": "^3.10.1",
    "three": "^0.181.1"
  }
}
