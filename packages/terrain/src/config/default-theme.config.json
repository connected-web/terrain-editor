{
  "locationMarkers": {
    "sprite": {
      "fontFamily": "\"DM Sans\", sans-serif",
      "fontWeight": "600",
      "maxFontSize": 52,
      "minFontSize": 22,
      "paddingX": 20,
      "paddingY": 10,
      "borderRadius": 18,
      "states": {
        "default": {
          "textColor": "#ffffff",
          "backgroundColor": "rgba(8, 10, 18, 0.78)",
          "borderColor": "rgba(255, 255, 255, 0.35)",
          "borderThickness": 2
        },
        "hover": {
          "backgroundColor": "rgba(8, 10, 18, 0.9)",
          "borderColor": "rgba(255, 255, 255, 0.6)"
        },
        "focus": {
          "textColor": "#111216",
          "backgroundColor": "rgba(255, 240, 218, 0.95)",
          "borderColor": "#f1d9a6",
          "borderThickness": 3
        }
      }
    },
    "stem": {
      "color": "#d9c39c",
      "hoverColor": "#f6dfb5",
      "focusColor": "#ffe7b5",
      "opacity": 0.75,
      "hoverOpacity": 0.95,
      "focusOpacity": 1
    }
  }
